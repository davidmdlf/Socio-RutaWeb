    <name>Socio-Ruta Web</name>
    <description>Visualizador de guiones sociales para personas con TEA</description>
    <author email="david.mdlf@gmail.com">David Miguel de la Fuente</author>
    <access origin="*"/>
    <preference name="fullscreen" value="true"/>
    <preference name="webviewbounce" value="true"/>
    
